{
  "name": "touristesim/touristesim-php-sdk",
  "description": "Official PHP SDK for Tourist eSIM Partner API - Easy integration for resellers and affiliates",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Tourist eSIM",
      "email": "developers@touristesim.net",
      "homepage": "https://touristesim.net"
    }
  ],
  "homepage": "https://github.com/touristesim/touristesim-php-sdk",
  "keywords": ["esim", "touristesim", "api", "sdk", "travel", "mobile", "data-plan", "international"],
  "require": {
    "php": ">=8.1",
    "guzzlehttp/guzzle": "^7.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "mockery/mockery": "^1.5",
    "phpstan/phpstan": "^1.0",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "TouristeSIM\\Sdk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TouristeSIM\\Sdk\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0-dev"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
